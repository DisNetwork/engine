{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "trailing-comma": false,
        "interface-name": false,
        "quotemark": false,
        "no-console": false,
        "no-trailing-whitespace": true,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-namespace": false,
        "variable-name": false
    },
    "rulesDirectory": []
}